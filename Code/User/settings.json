{
    "breadcrumbs.icons": false,

    "editor.detectIndentation": true,
    "editor.fontFamily": "monospace",
    "editor.rulers": [99],
    "editor.tabSize": 4,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.acceptSuggestionOnCommitCharacter": false,

    "explorer.autoReveal": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,

    "extensions.ignoreRecommendations": true,

    "diffEditor.ignoreTrimWhitespace": false,

    "files.autoSave": "onFocusChange",
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/.git": true,
    },
    "files.watcherExclude": {
        "**/.venv/**": true
    },

    "keyboard.dispatch": "keyCode",

    "search.mode": "reuseEditor",

    "terminal.integrated.shell.linux": "/bin/bash",

    "window.newWindowDimensions": "fullscreen",
    "window.openFoldersInNewWindow": "off",
    "window.restoreFullscreen": true,
    "window.restoreWindows": "none",
    "window.zoomLevel": 0,

    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Default Light+",
    "workbench.iconTheme": null,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tree.indent": 28,
    "workbench.tips.enabled": false,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.view.alwaysShowHeaderActions": true,


    "github.gitAuthentication": false,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.pullTags": true,
    "git.untrackedChanges": "separate",

    "projectManager.git.baseFolders": [
        "~/projects"
    ],
    "projectManager.git.maxDepthRecursion": 2,

    "rest-client.previewColumn": "current",

    "vim.leader": "<space>",
    "vim.normalModeKeyBindings": [
        {
            "before":["<leader>", "c", "c"], "after":[],
            "commands":["workbench.action.showCommands",],
        },
        {
            "before":["<leader>", "q", "q"], "after":[],
            "commands":["workbench.action.closeWindow",],
        },
        {
            "before":["<leader>", "f", "f"], "after":[],
            "commands":["breadcrumbs.toggleToOn",],
        },
        {
            "before":["<leader>", "g", "g"], "after":[],
            "commands":["workbench.scm.focus",],
        },
        {
            "before":["<leader>", "s", "s"], "after":[],
            "commands":["workbench.action.findInFiles"],
        },
        {
            "before":["<leader>", "r", "r"], "after":[],
            "commands":["rest-client.request",],
        },
        {
            "before":["<leader>", "p", "p"], "after":[],
            "commands":["projectManager.listProjects",],
        },
    ],
    "vim.visualModeKeyBindings": [],
    "vim.insertModeKeyBindings": [],
    "vim.normalModeKeyBindingsNonRecursive": [
        { "before":[">"], "after":[">",">"], "commands":[], },
        { "before":["<"], "after":["<","<"], "commands":[], },

    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        { "before":[">"], "after":[">","g","v"], "commands":[], },
        { "before":["<"], "after":["<","g","v"], "commands":[], },
    ],
    "vim.insertModeKeyBindingsNonRecursive": [],
    "vim.handleKeys": {
        "<C-w>": false,
        "<C-s>": false,
        "<C-c>": false,
        "<C-v>": true,
        "<C-x>": false,
    },

    "vim.easymotion": true,
    "vim.sneak": true,

    "vim.autoindent": false,
    "vim.useSystemClipboard": true,
    "vim.foldfix": true,
}
