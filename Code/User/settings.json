{
    "keyboard.dispatch": "keyCode",

    "editor.detectIndentation": true,
    "editor.fontFamily": "monospace",
    "editor.rulers": [99],
    "editor.tabSize": 4,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.acceptSuggestionOnCommitCharacter": false,

    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,

    "diffEditor.ignoreTrimWhitespace": false,

    "files.autoSave": "onFocusChange",
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/.git": true,
    },

    "terminal.integrated.shell.linux": "/bin/bash",

    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Default Light+",
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tree.indent": 28,

    "window.zoomLevel": 0,

    "extensions.ignoreRecommendations": true,

    "github.gitAuthentication": false,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.pullTags": true,
    "git.untrackedChanges": "separate",

    "gitProjectManager.maxDepthRecursion": 2,
    "gitProjectManager.baseProjectsFolders": [
        "~/Documents"
    ],

    "vim.leader": "<space>",
    "vim.normalModeKeyBindings": [
        {
            "before":["<leader>", "<leader>"], "after":[],
            "commands":["workbench.action.showCommands",],
        },
        {
            "before":["<leader>", "q", "q"], "after":[],
            "commands":["workbench.action.closeWindow",],
        },
        {
            "before":["<leader>", "q", "r"], "after":[],
            "commands":["workbench.action.reloadWindow",],
        },
        {
            "before":["<leader>", "f", "e"], "after":[],
            "commands":["workbench.view.explorer",],
        },
        {
            "before":["<leader>", "f", "f"], "after":[],
            "commands":["workbench.action.quickOpen",],
        },
        {
            "before":["<leader>", "f", "n"], "after":[],
            "commands":["extension.advancedNewFile",],
        },
        {
            "before":["<leader>", "g", "e"], "after":[],
            "commands":["workbench.scm.focus",],
        },
        {
            "before":["<leader>", "p", "p"], "after":[],
            "commands":["gitProjectManager.openProject",],
        },
    ],
    "vim.visualModeKeyBindings": [],
    "vim.insertModeKeyBindings": [],
    "vim.normalModeKeyBindingsNonRecursive": [
        { "before":[">"], "after":[">",">"], "commands":[], },
        { "before":["<"], "after":["<","<"], "commands":[], },

    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        { "before":[">"], "after":[">","g","v"], "commands":[], },
        { "before":["<"], "after":["<","g","v"], "commands":[], },
    ],
    "vim.insertModeKeyBindingsNonRecursive": [],
    "vim.handleKeys": {},
    "vim.autoindent": false,
    "vim.useSystemClipboard": true,
    "vim.foldfix": true,
}
